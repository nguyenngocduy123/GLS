{
    "name": "@simtech/vrp-nodejs",
    "version": "1.4.1",
    "private": true,
    "keywords": [
        "nodejs",
        "vehicle routing problem",
        "last-mile logistics"
    ],
    "author": "SIMTech VRP",
    "license": "SEE LICENSE IN LICENSE.md",
    "contributors": [
        "Joyce Tan <tandzj@simtech.a-star.edu.sg>",
        "Heng Chen Kim <hengck@simtech.a-star.edu.sg>"
    ],
    "engines": {
        "node": ">=8.10.0",
        "npm": ">6.4.0"
    },
    "scripts": {
        "start": "node main",
        "https": "node main https",
        "prepush": "npm run lint",
        "local": "node main --settings=setting-local.js --max-old-space-size=18000",
        "test": "jest --logHeapUsage --debug --detectOpenHandles",
        "test:full": "npm run test -- --watch",
        "lint": "eslint --fix --ignore-path .gitignore --ignore-path .eslintignore --ext .js . ",
        "db:migrate": "sequelize db:migrate --config vrp-sql/migrations/config/config.js --migrations-path vrp-sql/migrations",
        "db:migrate:local": "npm run db:migrate -- -- --settings=setting-local.js"
    },
    "dependencies": {
        "@mapbox/polyline": "1.0.0",
        "@simtech/vrp-frontend-gls": "1.3.6",
        "async": "2.6.1",
        "bcryptjs": "2.4.3",
        "bluebird": "3.5.2",
        "body-parser": "1.18.3",
        "compression": "1.7.3",
        "connect": "3.6.6",
        "connect-mongo": "2.0.1",
        "cookie-parser": "1.4.3",
        "cors": "2.8.4",
        "csurf": "1.9.0",
        "excel4node": "1.7.0",
        "express": "4.16.3",
        "express-session": "1.15.6",
        "generate-password": "1.4.0",
        "handlebars": "4.0.12",
        "helmet": "3.14.0",
        "jimp": "0.5.3",
        "jsonwebtoken": "8.3.0",
        "jszip": "3.1.5",
        "lodash": "4.17.11",
        "log4js": "3.0.6",
        "moment": "2.22.2",
        "mongodb": "3.1.8",
        "morgan": "1.9.1",
        "multer": "1.4.0",
        "node-schedule": "1.3.0",
        "nodemailer": "4.6.8",
        "optional-require": "1.0.0",
        "passport": "0.4.0",
        "passport-jwt": "4.0.0",
        "passport-local": "1.0.0",
        "pdfmake": "0.1.38",
        "proj4": "2.5.0",
        "request-promise": "4.2.2",
        "sequelize": "4.39.0",
        "sequelize-cli": "4.1.1",
        "socket.io": "2.1.1",
        "tedious": "2.6.4",
        "validator": "10.8.0"
    },
    "devDependencies": {
        "@simtech/vrp-driver-dist": "3.1.3",
        "eslint": "5.6.1",
        "eslint-config-strongloop": "2.1.0",
        "expect-more-jest": "2.3.0",
        "gulp": "4.0.0",
        "gulp-bump": "3.1.1",
        "gulp-debug": "4.0.0",
        "gulp-git": "2.8.0",
        "gulp-zip": "4.2.0",
        "husky": "1.1.0",
        "jest": "23.6.0",
        "jest-html-reporter": "2.4.2",
        "jsdoc": "3.5.5",
        "socket.io-client": "2.1.1",
        "supertest": "3.3.0",
        "yargs": "12.0.2"
    },
    "jest": {
        "globals": {
            "settings": "setting-local.js"
        },
        "verbose": true,
        "automock": false,
        "testEnvironment": "node",
        "testRegex": "/__test__/.*test.js$",
        "setupTestFrameworkScriptFile": "<rootDir>/__test__/setup.js",
        "reporters": [
            "default",
            [
                "./node_modules/jest-html-reporter",
                {
                    "pageTitle": "Test Report",
                    "outputPath": "./test-report.html",
                    "includeFailureMsg": true,
                    "includeConsoleLog": true
                }
            ]
        ]
    }
}
